\begin{enumerate}%
\def\figurename{Protokoll}%
\ifnum\@infdepth=0\else%        % For coding these lines, it took 2 hours. 
\@topsepadd\z@\@topsep\z@%      % I **really** like LaTeX ...
\fi%                            % ... especially it's good documentation
\advance\@infdepth\@ne%
\topsep\z@\parskip\z@\partopsep\z@\itemsep\z@\parsep\z@%
%
\let\block\infol\let\endblock\endinfol%
%
\def\acaption##1{%
\@ifnextchar [%
 {\@tempswatrue\@acaption{##1}{}}%
 {\@tempswafalse\@acaption{##1}{}[]}}%
\def\captionfrom##1##2{%
\@ifnextchar [%
 {\@tempswatrue\@acaption{##1}{##2}}%
 {\@tempswafalse\@acaption{##1}{##2}[]}}%
\def\@acaption##1##2[##3]{%
\def\@tempa{##2}%
\ifx\@tempa\@empty%
\caption[##1]{##1:\if@tempswa\\{\bf##3}\fi}%
\else%
\caption[##1]{##1\ (nach ##2):\if@tempswa\\{\bf##3}\fi}%
\fi}
%
\def\@captype{figure}%
\long\def\@makecaption##1##2{{%
\parshape=0\item[]##1: ##2\par}}
%
\def\theenumi{\arabic{enumi}}%
\def\labelenumi{\theenumi}%
\def\p@enumi{}%
%
\def\theenumii{\arabic{enumii}}%
\def\labelenumii{\theenumi.\theenumii}%
\def\p@enumii{\theenumi.}%
%
