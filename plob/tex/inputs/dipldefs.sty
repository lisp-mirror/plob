\def\p@enumiii{\theenumi.\theenumii.}%
%
\def\p@enumiv{\theenumi.\theenumii.\theenumiii.}%
}%

\def\endinfol{%
\advance\@infdepth\m@ne%
\end{enumerate}%
}

