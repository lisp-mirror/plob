   \setbox\@tempboxa\hbox{\reset@font##1: ##2}%
   \ifdim \wd\@tempboxa >\hsize%
       \hspace*{-\leftmargini}\parbox{\hsize}{\reset@font##1: ##2}%
     \else%
       \hspace*{-\leftmargini}{\hbox to\hsize{\hfil\box\@tempboxa\hfil}}%
   \fi}%
\begin{list}{}{}%				% KEIN trivlist benutzen!
